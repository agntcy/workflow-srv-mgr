{
  "name": "mailcomposer.with.deps",
  "version": "0.0.1",
  "schema_version": "0.3.1",
  "description": "Sample agent manifest with dependencies",  
  "authors": [
    "Cisco Systems"
  ],
  "created_at": "2025-05-21T00:00:00Z",
  "locators": [
    {
      "url": "github.com/agntcy/agentic-apps.git//mailcomposer",
      "type": "source-code"
    }
  ],
  "skills": [
    {
      "class_uid":10201
    }
  ],  
  "extensions": [
    {
      "name": "oasf.agntcy.org/feature/runtime/manifest",
      "version": "v0.0.1",
      "data": {
        "acp": {
          "input": {
            "type": "object",
            "properties": {
              "messages": {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "type": {
                      "type": "string",
                      "enum": [
                        "human",
                        "assistant"
                      ]
                    },
                    "content": {
                      "type": "string"
                    }
                  }
                }
              },
              "is_completed": {
                "type": "boolean"
              }
            }
          },
          "output": {
            "type": "object",
            "properties": {
              "messages": {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "type": {
                      "type": "string",
                      "enum": [
                        "human",
                        "assistant"
                      ]
                    },
                    "final_email": {
                      "type": "string"
                    }
                  }
                }
              },
              "is_completed": {
                "type": "boolean"
              }
            }
          },
          "config": {
            "type": "object",
            "description": "The configuration of the agent",
            "properties": {
              "test": {
                "type": "boolean"
              }
            }
          },
          "capabilities": {
            "threads": false,
            "interrupts": false,
            "callbacks": false
          },
          "interrupts": []
        },
        "deployment": {
          "agent_deps": [
            {
              "name": "email_reviewer_1",
              "ref": {
                "name": "email_reviewer",
                "version": "1.0.0",
                "url": "llama_manifest.json"
              },
              "deployment_option": "src",
              "env_var_values": {
                "values": {
                  "AZURE_OPENAI_MODEL": "gpt-4o-mini",
                  "TEST_ENV_VAR": "some test value"
                },
                "env_deps": []
              }
            }
          ],
          "deployment_options": [
            {
              "type": "source_code",
              "name": "src",
              "url": "github.com/agntcy/agentic-apps.git//mailcomposer",
              "framework_config": {
                "framework_type": "langgraph",
                "graph": "mailcomposer.mailcomposer:graph"
              }
            }
          ],
          "env_vars": [
            {
              "desc": "Environment variable for agentA",
              "name": "AZURE_OPENAI_MODEL",
              "required": true,
              "defaultValue": "gpt-4o-mini"        
            },
            {
              "desc": "Azure OpenAI API Key",
              "name": "AZURE_OPENAI_API_KEY",
              "required": true
            },
            {
              "desc": "Azure OpenAI Endpoint",
              "name": "AZURE_OPENAI_ENDPOINT",
              "required": true      
            },
            {
              "desc": "OpenAI API Version",
              "name": "OPENAI_API_VERSION",
              "required": true,
              "defaultValue": "2024-07-01-preview"
            }      
          ]
        }
      }
    }]
}
